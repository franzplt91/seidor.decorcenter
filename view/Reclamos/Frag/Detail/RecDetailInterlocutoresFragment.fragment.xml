<core:FragmentDefinition 
    height="100%" 
    xmlns:core="sap.ui.core" 
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns="sap.m"
    xmlns:l="sap.ui.layout" 
    xmlns:f="sap.ui.layout.form"
    xmlns:t="sap.ui.table" 
    xmlns:s="sap.m.semantic" 
    xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    displayBlock="true">
        
        <s:DetailPage title="Interlocutores" id="detail_rec_nuevo_interlocutores">
                <s:content>
                    <IconTabBar expandable="false" headerMode="Inline" upperCase="false" expanded="{device&gt;/isNoPhone}" class="sapUiResponsiveContentPadding">
                        <items>
                            <IconTabFilter showAll="false" text="Cliente">


                                <f:Form editable="true">
                            <f:layout>
                                <f:ResponsiveGridLayout
                                    labelSpanXL="3"
                                    labelSpanL="3"
                                    labelSpanM="3"
                                    labelSpanS="6"
                                    adjustLabelSpan="false"
                                    emptySpanXL="4"
                                    emptySpanL="4"
                                    emptySpanM="4"
                                    emptySpanS="0"
                                    columnsXL="1"
                                    columnsL="1"
                                    columnsM="1"
                                    singleContainerFullSize="false" />
                            </f:layout>
                            <f:formContainers>
                                <f:FormContainer>
                                    <f:formElements>
                                        <f:FormElement label="Función">
                                            <f:fields>
                                                <Input width="100%" id="txt_funcion" 
                                                    value="{/objReclamo/Interlocutor/AG/PARVW}" >
                                                </Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Código">
                                            <f:fields>
                                                <Input width="100%" id="txt_codigo"
                                                 value="{/objReclamo/Interlocutor/AG/KUNNR}">  
                                                </Input>
                                            </f:fields>
                                        </f:FormElement>                   
                                        <f:FormElement label="Nombre">
                                            <f:fields>
                                                <Input width="100%" id="txt_nombre" 
                                                    value="{/objReclamo/Interlocutor/AG/NOMBRE}">
                                                </Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Dirección">
                                            <f:fields>
                                                <Input width="100%" id="txt_direccion" 
                                                    value="{/objReclamo/Interlocutor/AG/Direccion}">
                                                </Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Ubicación">
                                            <f:fields>
                                                <ComboBox items="{ path : '/dataIni/lstZipCodes'}" 
                                                    change="onSeleccionaSurtido" maxWidth="100%" required="true"
                                                    id="cbo_ubicacion" selectedKey="{/objReclamo/Interlocutor/AG/Ciudad}">
                                                    <core:Item key="{Codigo}" text="{Descripcion}" />
                                                </ComboBox>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Teléfono">
                                            <f:fields>
                                                <Input width="100%" placeholder="Ingrese Teléfono" id="txt_telefono" 
                                                    value="{/objReclamo/Interlocutor/AG/Telefono}">
                                                </Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="NIF">
                                            <f:fields>
                                                <Input width="100%" placeholder="Ingrese NIF" id="txt_nif" 
                                                    value="{/objReclamo/Interlocutor/AG/NIF}">
                                                </Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Correo">
                                            <f:fields>
                                                <Input width="100%" placeholder="Ingrese Correo" id="txt_correo" 
                                                    value="{/reclamo/mail}">       
                                                </Input>
                                            </f:fields>
                                        </f:FormElement>
                                        
                                    </f:formElements>
                                </f:FormContainer>
                            </f:formContainers>
                        </f:Form>


                                
                            </IconTabFilter>
                            <IconTabFilter showAll="false" text="Persona de Contacto">

                                <f:Form editable="true">
                            <f:layout>
                                <f:ResponsiveGridLayout
                                    labelSpanXL="3"
                                    labelSpanL="3"
                                    labelSpanM="3"
                                    labelSpanS="6"
                                    adjustLabelSpan="false"
                                    emptySpanXL="4"
                                    emptySpanL="4"
                                    emptySpanM="4"
                                    emptySpanS="0"
                                    columnsXL="1"
                                    columnsL="1"
                                    columnsM="1"
                                    singleContainerFullSize="false" />
                            </f:layout>
                            <f:formContainers>
                                <f:FormContainer>
                                    <f:formElements>
                                        <f:FormElement label="Función">
                                            <f:fields>
                                                <Input width="100%" id="txt_funcion_persona_contacto" value="AP"></Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Nombre">
                                            <f:fields>
                                                <Input width="100%" placeholder="Ingrese nombre" id="txt_nombre_persona_contacto" value="{/objReclamo/Z_Reclamo/PCONTACTO}"></Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Dirección">
                                            <f:fields>
                                                <Input width="100%" placeholder="Ingrese Direccón" id="txt_direccion_persona_contacto" value="{/objReclamo/Z_Reclamo/DIRECCION}"></Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Ubicación">
                                            <f:fields>
                                                <ComboBox items="{ path : '/dataIni/lstZipCodes' }" change="onSeleccionaSurtido" maxWidth="50%" required="true" id="cbo_ubicacion_persona_contacto" selectedKey="{/objReclamo/Z_Reclamo/CPOSTAL}">
                                                    <core:Item key="{Codigo}" text="{Descripcion}" />
                                                </ComboBox>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Teléfono">
                                            <f:fields>
                                                <Input width="100%" placeholder="Ingrese teléfono" 
                                                    id="txt_telefono_persona_contacto" 
                                                    value="{/objReclamo/Z_Reclamo/TELEFONO}">
                                                </Input>
                                            </f:fields>
                                        </f:FormElement>
                                        
                                    </f:formElements>
                                </f:FormContainer>
                            </f:formContainers>
                        </f:Form>

                               
                            </IconTabFilter>
                            <IconTabFilter showAll="false" text="Código de Asesor">

                                <f:Form editable="true">
                            <f:layout>
                                <f:ResponsiveGridLayout
                                    labelSpanXL="3"
                                    labelSpanL="3"
                                    labelSpanM="3"
                                    labelSpanS="6"
                                    adjustLabelSpan="false"
                                    emptySpanXL="4"
                                    emptySpanL="4"
                                    emptySpanM="4"
                                    emptySpanS="0"
                                    columnsXL="1"
                                    columnsL="1"
                                    columnsM="1"
                                    singleContainerFullSize="false" />
                            </f:layout>
                            <f:formContainers>
                                <f:FormContainer>
                                    <f:formElements>
                                        <f:FormElement label="Función">
                                            <f:fields>
                                                <Input width="100%" id="txt_funcion_codigo_asesor" 
                                                    value="{/objReclamo/Interlocutor/ZM/PARVW}">
                                                </Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Código">
                                            <f:fields>
                                                <Input width="100%" id="txt_codigo_asesor" 
                                                    value="{/objReclamo/Interlocutor/ZM/PERNR}">
                                                </Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Nombre">
                                            <f:fields>
                                                <Input width="100%" id="txt_nombre_codigo_asesor" 
                                                    value="{/objReclamo/Interlocutor/ZM/NOMBRE}">        
                                                </Input>
                                            </f:fields>
                                        </f:FormElement>
                                        
                                        
                                    </f:formElements>
                                </f:FormContainer>
                            </f:formContainers>
                        </f:Form>

                                
                            </IconTabFilter>
                            <IconTabFilter showAll="false" text="Enc. Comercial">


                                <f:Form editable="true">
                            <f:layout>
                                <f:ResponsiveGridLayout
                                    labelSpanXL="3"
                                    labelSpanL="3"
                                    labelSpanM="3"
                                    labelSpanS="6"
                                    adjustLabelSpan="false"
                                    emptySpanXL="4"
                                    emptySpanL="4"
                                    emptySpanM="4"
                                    emptySpanS="0"
                                    columnsXL="1"
                                    columnsL="1"
                                    columnsM="1"
                                    singleContainerFullSize="false" />
                            </f:layout>
                            <f:formContainers>
                                <f:FormContainer>
                                    <f:formElements>
                                        <f:FormElement label="Función">
                                            <f:fields>
                                                <Input width="100%" id="txt_funcion_comercial" value="VE"></Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Código">
                                            <f:fields>
                                                <Input width="100%" placeholder="Ingrese código"
                                                 id="txt_codigo_empresa_comercial" 
                                                 value="{/objReclamo/Interlocutor/VE/KUNNR}">
                                                 </Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Nombre">
                                            <f:fields>
                                                <Input width="100%" placeholder="Ingrese Nombre" 
                                                    id="txt_nombre_empresa_comercial" 
                                                    value="{/objReclamo/Interlocutor/VE/NOMBRE}">
                                                </Input>
                                            </f:fields>
                                        </f:FormElement>
                                        
                                        
                                    </f:formElements>
                                </f:FormContainer>
                            </f:formContainers>
                        </f:Form>

                                
                            </IconTabFilter>
                        </items>
                    </IconTabBar>
                </s:content>
                <s:customFooterContent>
                    <Button text="Copiar Datos" press="onCopiarCliente"></Button>
                    <Button text="Buscar" icon="sap-icon://search" press="onDocNuevoBuscarCliente"></Button>
                </s:customFooterContent>
            </s:DetailPage>
        
    </core:FragmentDefinition>